<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pro">
	<select id="gogekList" resultType="dto">
		select gogek_no, gogek_name, sawon_name, sawon_jik, buser_name, buser_Tel from gogek
		left join sawon on gogek_damsano=sawon_no
		inner join buser on buser_no = buser_num order by gogek_no;
	</select>
	
	<select id="gogekListSearch">
		select gogek_no, gogek_name, sawon_name, sawon_jik, buser_name, buser_Tel from gogek
		left join sawon on gogek_damsano=sawon_no
		inner join buser on buser_no = buser_num where gogek_no=1 and gogek_name='이나라';
	</select>
	
</mapper>